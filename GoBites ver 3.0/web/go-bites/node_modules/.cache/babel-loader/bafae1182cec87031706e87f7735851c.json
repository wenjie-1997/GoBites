CAAA;AACA;AACA;;AACA,UAAA,EAAA,KAAA,YAAA,KAAA,SAAA,KAAA,EAAA,KAAA,YAAA,CAAA,MAAA,IAAA,CAAA,CAAA,EAAA;AACA,aAAA,eAAA,GAAA,KAAA,YAAA;AACA,aAAA,YAAA,GAAA,EAAA;AACA;AACA,KAnDA;AAoDA,IAAA,KApDA,mBAoDA;AACA,WAAA,UAAA,GAAA,KAAA,MAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,UAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,UAAA,CAAA,MAAA,GAAA,IAAA;AACA,aAAA,kBAAA,CAAA,MAAA,CAAA,CAAA,EAAA,KAAA,kBAAA,CAAA,MAAA;AACA;AACA,KA1DA;AA2DA,IAAA,iBA3DA,6BA2DA,SA3DA,EA2DA;AACA,UAAA,KAAA,kBAAA,EAAA;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,kBAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,KAAA,kBAAA,CAAA,CAAA,MAAA,SAAA,EAAA;AACA,iBAAA,kBAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA;AACA;AACA;AACA,KApEA;AAqEA,IAAA,oBArEA,gCAqEA,SArEA,EAqEA;AACA,WAAA,kBAAA,CAAA,IAAA,CAAA,SAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,kBAAA;AACA;AAxEA,GAvCA;AAiHA,EAAA,OAjHA,qBAiHA;AACA,SAAA,SAAA,GAAA,IAAA;AACA,SAAA,4BAAA;AACA;AApHA,CAAA","sourcesContent":["<template>\n<div id=\"orderList\">\n    <div v-if='isLoading'>\n        <h3>Retrieving data...</h3>\n        <loading v-if='isLoading' />\n    </div>\n\n    <div v-if='!(orders === undefined) && !isLoading'>\n        <div class=\"row\" v-if=\"ordersData.length > 0\">\n            <div class=\"col-md-4\">\n                <Search @displaySearchResult=\"displaySearchResult\" @reset=\"reset\" />\n            </div>\n            <div class=\"col-md-7\">\n                <DisplayWithFilter @unselectCondition=\"unselectCondition\" @setSelectedCondition=\"setSelectedCondition\" :requiredConditions=\"conditions\" />\n            </div>\n        </div>\n        <DisplayInformation v-if=\"orders\" @viewUserDetails=\"viewUserDetails\" :userType=\"userType\" :users=\"ordersData\" :userLabels=\"orderLabels\" :unWantedProperty=\"unWantedProperty\" />\n    </div>\n</div>\n</template>\n\n<script>\nimport OrderDataService from '../services/OrderDataService';\nimport loading from '../mixins/loading.vue';\nimport DisplayInformation from '../components/DisplayInformation.vue';\nimport Search from '../components/Search.vue';\nimport DisplayWithFilter from '../components/DisplayWithFilter.vue';\n\nexport default {\n    name: 'OrderList',\n    components: {\n        loading,\n        DisplayInformation,\n        Search,\n        DisplayWithFilter,\n    },\n    data() {\n        return {\n            ordersData: [],\n            orders: [],\n            isLoading: false,\n            unWantedProperty: 'fk_mlid',\n            userType: 'orders',\n\n            searchResult: [],\n\n            conditions: [\n                {\n                    condition :\"Order ID\",\n                    toggle :\"no\",\n                },\n                {\n                    condition :\"Customer ID\",\n                    toggle :\"no\",\n                },\n            ],\n\n            selectedConditions: [],\n\n            orderLabels: [\n                {label: \"Order ID\"},\n                {label: \"Total Price\"},\n                {label: \"Customer ID\"},\n                {label: \"Manage Order\"},\n            ]\n        }\n    },\n    methods: {\n        async retrieveAllOrdersInformation() {\n            await OrderDataService.getAllOrders()\n                .then(response => {\n                    this.isLoading = false;\n                    for (var i = 0; i < Object.keys(response.data).length; i++) {\n                        this.orders.push(response.data[i]);\n                    }\n                    this.ordersData = this.orders;\n                }).catch(err => {\n                    alert(err.message);\n                });\n        },\n        viewUserDetails: function (order) {\n            console.log(\"viewUserDetails is not used yet... \" + order);\n        },\n        displaySearchResult(requiredInformation) {\n            if(!(this.selectedConditions === undefined) && !(this.selectedConditions.length == 0)) {\n                for(var i = 0; i < this.selectedConditions.length; i++) {\n                    if(this.selectedConditions[i] === \"Order ID\") {\n                        this.searchByOrderID(requiredInformation);\n                    }\n                    else if(this.selectedConditions[i] === \"Customer ID\") {\n                        this.searchByCustomerID(requiredInformation);\n                    }\n                }\n            }else {\n                alert(\"No filter had been chosen\");\n            }\n        },\n        searchByOrderID(requiredInformation) {\n            for(var i = 0; i < this.orders.length; i++) {\n                if(this.orders[i].orderid == requiredInformation) {\n                    this.searchResult.push(this.orders[i]);\n                }\n            }\n            if(!(this.searchResult === undefined) && !(this.searchResult.length == 0)){\n                this.ordersData = this.searchResult;\n                this.searchResult = [];\n            }\n        },\n        searchByCustomerID(requiredInformation) {\n            for(var i = 0; i < this.orders.length; i++) {\n                if(this.orders[i].fk_cid == requiredInformation) {\n                    this.searchResult.push(this.orders[i]);\n                }\n            }\n            if(!(this.searchResult === undefined) && !(this.searchResult.length == 0)){\n                this.restaurantsData = this.searchResult;\n                this.searchResult = [];\n            }\n        },\n        reset() {\n            this.ordersData = this.orders;\n            for (var i = 0; i < this.conditions.length; i++) {\n                this.conditions.toggle = \"no\";\n                this.selectedConditions.splice(0, this.selectedConditions.length);\n            }\n        },\n        unselectCondition(condition) {\n            if(this.selectedConditions) {\n                for (var i = 0; i < this.selectedConditions.length; i++) {\n                    if(this.selectedConditions[i] === condition) {\n                        this.selectedConditions.splice(i, 1);\n                        break;\n                    }\n                }\n            }\n        },\n        setSelectedCondition(condition) {\n            this.selectedConditions.push(condition);\n            console.log(this.selectedConditions);\n        },\n    },\n    mounted() {\n        this.isLoading = true;\n        this.retrieveAllOrdersInformation();\n    },\n}\n</script>\n\n<style lang=\"scss\">\n#orderList {\n    width: 80%;\n    position: relative;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    -ms-transform: translate(-50%, -50%);\n    /* IE 9 */\n    -webkit-transform: translate(-50%, -50%);\n    /* Chrome, Safari, Opera */\n\n    padding-top: 5%;\n    padding-bottom: 5%;\n}\n\n</style>\n"],"sourceRoot":"src/views"}]}